{
    "name": "uzsoftic/laravel-eskiz",
    "description": "Laravel Eskiz SMS",
    "license": "MIT",
    "authors": [
        {
            "name": "Umarov Kamoliddin",
            "email": "softuzb@gmail.com",
            "homepage": "https://softic.uz"
        }
    ],
    "minimum-stability": "dev",
    "homepage": "https://github.com/uzsoftic/laravel-eskiz",
    "keywords": ["Laravel", "LaravelEskiz"],
    "require": {
        "illuminate/support": "~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Uzsoftic\\LaravelEskiz\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uzsoftic\\LaravelEskiz\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uzsoftic\\LaravelEskiz\\LaravelEskizServiceProvider"
            ],
            "aliases": {
                "LaravelEskiz": "Uzsoftic\\LaravelEskiz\\Facades\\LaravelEskiz"
            }
        }
    }
}
